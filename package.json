{
  "name": "upload-with-progress",
  "version": "1.0.2",
  "author": "Abhishek Singh <official.6packprogrammer@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meabhisingh/upload-with-progress.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "devDependencies": {
    "@types/bun": "^1.2.20",
    "fast-glob": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.10",
    "typedoc-plugin-markdown": "^4.9.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "typescript": ">=5"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "import": "./dist/browser/index.js"
    }
  },
  "bugs": "https://github.com/meabhisingh/upload-with-progress/issues",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/meabhisingh/upload-with-progress.git#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun ./builder.ts && bun run generate-types",
    "prepublishOnly": "bun run build",
    "generate-types": "bunx tsc --emitDeclarationOnly --declaration --outDir dist",
    "clear": "rm -rf dist",
    "docs": "typedoc --out docs/pages/api-reference src "
  },
  "type": "module",
  "types": "./dist/index.d.ts"
}
